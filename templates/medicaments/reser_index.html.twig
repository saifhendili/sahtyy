{% extends 'base.html.twig' %}

{% block title %}ReservationMed index{% endblock %}

{% block section %}
<section class="mypage"></section>
    <h1>ReservationMed index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>IdMed</th>
                <th>IdPhar</th>
                <th>IdPatient</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for reservation_med in reservation_meds %}
            <tr>
                <td>{{ reservation_med.id }}</td>
                <td>{{ reservation_med.idMed }}</td>
                <td>{{ reservation_med.idPhar }}</td>
                <td>{{ reservation_med.idPatient }}</td>
                <td>
                    <a href="{{ path('reservation_med_show', {'id': reservation_med.id}) }}">show</a>
                    <a href="{{ path('reservation_med_edit', {'id': reservation_med.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>


{% endblock %}
